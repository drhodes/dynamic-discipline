{
    "name": "dynamic-discipline-svg",
    "version": "1.0.0",
    "description": "a visualization of the dynamic discipline",
    "private": true,
    "type": "module",
    "scripts": {
        "bundle": "make bundle",
        "watch": "npm-watch",
        "test": "mocha"
    },
    "author": "Derek Rhodes <physci@gmail.com> (https://sdf.org/~rhoder)",
    "license": "MIT",
    "devDependencies": {
        "mocha": "^8.4.0",
        "webpack": "^5.38.1",
        "webpack-cli": "^4.7.2"
    },
    "dependencies": {
        "@svgdotjs/svg.js": "^3.1",
        "esm": "^3.2.25",
        "lodash": "^4.17.21",
        "npm-watch": "^0.9.0"
    },
    "watch": {
        "bundle": {
            "patterns": [
                "src",
                "dist/index.html"
            ],
            "extensions": "js, html",
            "delay": "500",
            "runOnChangeOnly": false
        }
    },
    "keywords": [
        "computer",
        "engineering",
        "educational"
    ]
